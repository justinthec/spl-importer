<div class="container">
  <div class="row text-center">
    <div class="page-header">
      <h1>Import Successful! <small><b class="text-success"><%= @matches.count %></b> matches added</small></h1>
    </div>
    <h4><p>Calendar: <em><b class="text-info">Starcraft 2 Proleague</b></em> created</p></h4>
    <ul class="list-group">
      <% @matches.each do |match| %>
        <li class="list-group-item"><em><%= match.info %></em></li>
      <% end %>
    </ul>
  </div>
</div>